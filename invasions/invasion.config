# Standard configuration file for invasion scenarios
# Daniel Vedder, 30/04/2018
#        updated 22/01/2018

# software parameters
seed 0 #random seed
maps "invasion.map"
dest "results/invasion"
outfreq 50
logging true
debug true
fasta false
lineages true
raw false

# general model parameters
linkage "none"
nniches 2
tolerance "none"
static false
cellsize 2e6
mutate false
usebiggenes false
indsize "mixed"
initpopsize "metabolic"
minseedsize 0
minrepsize 5

# invasion parameters
propagule-pressure 0 #1
global-species-pool 0 #100
disturbance 1
burn-in 500
