# Configuration file for island diversification experiment tests
 
# software parameters
maps islsim_test.map
outfreq 25
logging true
debug false
fasta false
lineages false
dest results/islsim_test
seed 0

# general model parameters
allometry true
linkage random
maxloci 20
nniches 2
static true
cellsize 1e5
mutate true
fixtol false
mintemp 290
maxtemp 302
maxrepsize 12 # just above 100kg, and thus K
mutationrate 3.6e12 #100 times as much as default
dispshape 10.0
phylconstr 0.5
overfill 10
# mortality 1.3188157344832146e9 #exp(21)

disturbance 0
indsize seed
usebiggenes true
raw true
