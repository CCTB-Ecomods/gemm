# Configuration file for island diversification experiments

# software parameters
maps island.map
outfreq 500
logging true
debug false
fasta all
lineages true
dest output/2020-05-02

# general model parameters
linkage random
maxloci 20
nniches 2
static true
cellsize 1e5
mutate true
fixtol false
mintemp 290
maxtemp 302
maxrepsize 12 # just above 100kg, and thus K
dispshape 10.0
phylconstr 0.5
overfill 10
# mortality 1.3188157344832146e9 #exp(21)

disturbance 0
indsize seed
usebiggenes true
raw true
