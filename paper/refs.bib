@article{friisGenomewideSignalsDrift2018,
  langid = {english},
  title = {Genome-Wide Signals of Drift and Local Adaptation during Rapid Lineage Divergence in a Songbird},
  url = {https://www.biorxiv.org/content/early/2018/01/07/243766},
  doi = {10.1101/243766},
  abstract = {The formation of independent evolutionary lineages involves neutral and selective factors, and understanding their relative roles in population divergence is a fundamental goal of speciation research. Correlations between allele frequencies and environmental variability can reveal the role of selection, yet the relative contribution of drift can be difficult to establish. Recently diversified systems such as that of the Oregon junco (Aves: Emberizidae) of western North America provide ideal scenarios to apply genetic-environment association analyses (GEA) while controlling for population structure. Genome-wide SNP loci analyses revealed marked genetic structure consisting of differentiated populations in isolated, dry southern mountain ranges, and more admixed recently expanded populations in humid northern latitudes. We used correlations between genomic and environmental variance to test for three specific modes of evolutionary divergence: (i) drift in geographic isolation, (ii) differentiation along continuous selective gradients, and (iii) isolation by adaptation. We found evidence of strong drift in southern mountains, but also signals of local adaptation in several populations, driven by temperature, precipitation, elevation and vegetation, especially when controlling for population history. We identified numerous variants under selection scattered across the genome, suggesting that local adaptation can promote rapid differentiation over short periods when acting over multiple independent loci.},
  journaltitle = {bioRxiv},
  urldate = {2018-05-25},
  date = {2018-01-07},
  pages = {243766},
  author = {Friis, Guillermo and Fandos, Guillermo and Zellmer, Amanda and McCormack, John and Faircloth, Brant and Mila, Borja},
  file = {/home/ludwig/.mozilla/firefox/li70n5dq.default/zotero/storage/NCVS5JBB/Friis et al. - 2018 - Genome-wide signals of drift and local adaptation .pdf;/home/ludwig/.mozilla/firefox/li70n5dq.default/zotero/storage/9FMHKFWU/243766.html}
}

