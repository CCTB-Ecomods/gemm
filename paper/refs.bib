@article{schneiderharaldNeoPaleopolyploidyContribute2017,
  title = {Neo‐ and {{Paleopolyploidy}} Contribute to the Species Diversity of {{Asplenium}}—the Most Species‐rich Genus of Ferns},
  volume = {55},
  issn = {1674-4918},
  url = {https://onlinelibrary.wiley.com/doi/full/10.1111/jse.12271},
  doi = {10.1111/jse.12271},
  abstract = {Abstract Polyploidy is widely considered as a major process in the evolution of plants but the accumulation of polyploid species diversity is still controversial. Some recent studies proposed increased extinction risk in neopolyploids compared with their diploid ancestors. The high proportion of polyploid ferns is expected to be formed mainly by neopolyploids, whereas paleopolyploid species are predicted to be clustered in clades founded by whole genome duplications. Here, we test this prediction by exploring the evolution of polyploidy in the derived fern family Aspleniaceae. The family has a global distribution and shows the highest frequency of polyploid taxa among all ferns. To test the hypothesis, we obtained a comprehensive phylogeny using chloroplast DNA sequences of 883 specimens representing 292 species. All published chromosome counts were mapped onto this phylogenetic framework in order to explore the evolution of polyploids. We recovered evidence for several whole genome duplications in the history of Aspleniaceae. Phylogenetic relationships of polyploids exceeding the tetraploid level suggest that tetraploid Asplenium species may have replaced their diploid ancestors as the main evolutionary players in some clades of this family.},
  number = {4},
  journaltitle = {Journal of Systematics and Evolution},
  shortjournal = {Journal of Systematics and Evolution},
  urldate = {2018-04-05},
  date = {2017-07-21},
  pages = {353-364},
  keywords = {macroevolution,extinction risk,diversification,chromosome number,genome evolution,neopolyploidy,paleopolyploidy},
  author = {{Schneider Harald} and {Liu Hong‐Mei} and {Chang Yan‐Fen} and {Ohlsen Daniel} and {Perrie Leon R.} and {Shepherd Lara} and {Kessler Michael} and {Karger Dirk N.} and {Hennequin Sabine} and {Marquardt Jeannine} and {Russell Stephen} and {Ansell Stephen} and {Lu Ngan Thi} and {Kamau Peris} and {Lóriga Josmaily} and {Regalado Ledis} and {Heinrichs Jochen} and {Ebihara Atsushi} and {Smith Alan R.} and {Gibby Mary}},
  file = {/home/ludwig/.mozilla/firefox/li70n5dq.default/zotero/storage/677FJWR6/Schneider Harald et al. - 2017 - Neo‐ and Paleopolyploidy contribute to the species.pdf;/home/ludwig/.mozilla/firefox/li70n5dq.default/zotero/storage/VCDLELNL/jse.html}
}

@article{richardsonRapidDiversificationSpeciesRich2001,
